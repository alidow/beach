{
  "name": "@beach/private-beach",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:tile-resize": "playwright test tests/e2e/tile-resize-hud.spec.ts --workers=1 --timeout=90000",
    "test:e2e:tile-resize:ui": "playwright test tests/e2e/tile-resize-hud.spec.ts --ui",
    "test:e2e:tile-resize:debug": "PWDEBUG=1 playwright test tests/e2e/tile-resize-hud.spec.ts",
    "test:e2e:resize": "playwright test tests/e2e/tile-resize-hud",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:perf": "tsx tests/scripts/canvas-perf.ts",
    "postinstall": "node scripts/copy-wasm.js",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit push --config drizzle.config.ts"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.33.7",
    "argon2-browser": "^1.18.0",
    "drizzle-orm": "^0.32.2",
    "next": "14.2.25",
    "next-themes": "^0.4.6",
    "noise-c.wasm": "^0.4.0",
    "pg": "^8.11.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-grid-layout": "^1.4.4",
    "react-resizable": "^3.0.5",
    "reactflow": "^11.10.4"
  },
  "devDependencies": {
    "@clerk/clerk-sdk-node": "^4.13.23",
    "@playwright/test": "^1.48.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "20.11.17",
    "@types/pg": "^8.15.5",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-resizable": "^3.0.4",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.26.0",
    "eslint": "8.56.0",
    "eslint-config-next": "14.2.25",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.20.6",
    "typescript": "5.3.3",
    "vitest": "^1.6.0"
  }
}
